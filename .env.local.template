################################################################################
# EAGLE EYE LOCAL DEVELOPMENT ENVIRONMENT
# 
# THIS FILE IS GIT-IGNORED FOR SECURITY - NEVER COMMIT
# Copy values from .env.example and customize for your local setup
################################################################################

ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# Local database
DATABASE_URL=postgresql+psycopg://eagle:eagle@localhost:5432/eagle

# Local MinIO (docker-compose)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minio
S3_SECRET_KEY=minio123
S3_BUCKET=eagle-files

# Local Redis
REDIS_URL=redis://localhost:6379/0

# YOUR OpenAI API Key (from https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-proj-YOUR_KEY_HERE

# Optional: Local Ollama for testing
OLLAMA_ENABLED=false
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# Optional: Hugging Face for testing
HUGGINGFACE_ENABLED=false
HUGGINGFACE_API_KEY=hf_YOUR_TOKEN_HERE
HUGGINGFACE_MODEL=gpt2
HUGGINGFACE_TASK=text-generation

# Local settings
DEFAULT_STATE=GA
CODE_SET=IRC2018_IECC2015_NEC2017_GA

# Local API server
API_HOST=0.0.0.0
API_PORT=8000

# n8n local
N8N_ENABLED=true
N8N_WEBHOOK_URL=http://localhost:5678/webhook
N8N_API_URL=http://localhost:5678/api

# Optional: Local Seq for logging
SEQ_ENABLED=false
SEQ_URL=http://localhost:5341
